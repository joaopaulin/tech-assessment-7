{
  "name": "kirvano-technical-assessment-7",
  "version": "v1.0.0",
  "main": "app.js",
  "license": "UNLICENSED",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "dev:debug": "tsx --inspect-brk src/app.ts",
    "build": "rm -Rf dist && tsc --project tsconfig-build.json && yarn tscpaths -p tsconfig.json -s ./src -o ./build/src && cp -Rp ./build/src ./dist",
    "start": "node dist/app.js",
    "test": "vitest --config ./vitest.config.ts run",
    "test:watch": "vitest --config ./vitest.config.ts",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "resolutions": {
    "micromatch": ">=4.0.8",
    "braces": ">=3.0.3",
    "esbuild": ">=0.25.0",
    "vite": ">=5.4.20",
    "axios": ">=1.12.0",
    "form-data": ">=4.0.4"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "dotenv-expand": "^11.0.6",
    "express": "^4.21.2",
    "helmet": "^7.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security-node": "^1.1.1",
    "prettier": "^2.7.1",
    "tscpaths": "^0.0.9",
    "tsup": "^8.1.0",
    "tsx": "^4.15.7",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0"
  }
}
